{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "hubName": "testv1",
      "useGracefulShutdown": true,
      "maxConcurrentActivityFunctions": 5,
      "maxConcurrentOrchestratorFunctions": 5,
      "storageProvider": {
        "type": "Netherite"
      }
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  }
}